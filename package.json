{
  "name": "@taiyuuki/utils",
  "version": "0.4.0",
  "description": "Utils by Taiyuuki",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/taiyuuki/utils"
  },
  "bugs": "https://github.com/taiyuuki/utils/issues",
  "homepage": "https://github.com/taiyuuki/utils#readme",
  "scripts": {
    "lint": "eslint --ext .js,.ts ./ --fix",
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "test": "vitest",
    "doc": "npx api-extractor run --local --verbose && npx api-documenter markdown -i temp -o doc",
    "doc debug": "npx api-extractor run --local --verbose --diagnostics && npx api-documenter markdown -i temp -o doc",
    "trace": "npx tsc dist/types/index.d.ts --traceResolution > trace.log",
    "publish": "pnpm publish --registry=https://registry.npmjs.org --access=public"
  },
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "author": "taiyuuki <taiyuuki@qq.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@microsoft/api-documenter": "^7.22.7",
    "@microsoft/api-extractor": "^7.35.1",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@taiyuuki/eslint-config-ts": "^0.0.9",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^18.16.16",
    "eslint": "^8.41.0",
    "jsdom": "^21.1.2",
    "rollup": "^2.79.1",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-import-export": "^0.0.1",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.5",
    "vitest": "^0.24.5"
  }
}
